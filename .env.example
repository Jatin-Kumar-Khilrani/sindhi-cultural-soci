# Azure Cosmos DB Configuration
# Copy this file to .env.local and fill in your values

# Your Cosmos DB account endpoint (from Azure Portal -> Cosmos DB -> Keys)
VITE_COSMOS_ENDPOINT=https://your-account.documents.azure.com:443/

# Primary or Secondary key from Azure Portal -> Cosmos DB -> Keys
VITE_COSMOS_KEY=your-primary-key-here

# Database name (will be created if it doesn't exist)
VITE_COSMOS_DATABASE=sindhi-cultural-db

# Container name (will be created if it doesn't exist)
VITE_COSMOS_CONTAINER=kv-store
